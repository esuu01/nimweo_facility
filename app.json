{
  "expo": {
    "name": "Nimweo Facility",
    "slug": "nimweo-facility",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esuu01.nimweofacility",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos and videos.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record videos with sound.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save photos and videos.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to your photo library to save photos and videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEOS"
      ],
      "package": "com.esuu01.nimweofacility"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "abba33b4-a2eb-4dc9-8c73-a19235dd287e"
      }
    },
    "owner": "esuu01",
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(PRODUCT_NAME) needs access to your Microphone."
        }
      ]
    ],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/abba33b4-a2eb-4dc9-8c73-a19235dd287e"
    }
  }
}
